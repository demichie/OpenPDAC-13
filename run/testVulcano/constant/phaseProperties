FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (particles gas);

continuousPhase gas;

pMin 10000;

particles
{
    type          purePhaseModel;
    diameterModel constant;
    constantCoeffs
    {
        d               1e-4;
    }

    alphaMax        0.67;
    residualAlpha   1e-8;
}

gas
{
    type          multiComponentPhaseModel; //purePhaseModel;
    diameterModel none;

    residualAlpha   1e-8;
}

blending
{
    default
    {
        type            continuous;
        phase gas;
    }
}

surfaceTension
{
    particles_gas
    {
        type            constant;
        sigma           0;
    }
}


phaseTransfer
{}

lift
{}

wallLubrication
{}

turbulentDispersion
{}

interfaceCompression
{}

